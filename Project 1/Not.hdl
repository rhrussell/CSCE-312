/**
 * Not gate:
 * out = not in
 */

 /*
    Truth Table
    in      out
    0       1
    1       0
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Put your code here:
    Nor(a=in,b=in,out=out);
}